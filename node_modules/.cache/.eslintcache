function resetInput(e) {\n    e.preventDefault()\n    {\n      inputRef.current.value = 0\n    }\n  }\n\n  function resetResult(e) {\n    e.preventDefault()\n    setResult((result) => result * 0)\n  }\n\n  return (\n    <div className='App'>\n      <div>\n        <h1>Simplest Working Calculator</h1>\n      </div>\n\n      <form>\n        <p ref={resultRef}>\n          {result}\n        </p>\n\n        <input\n          pattern='[0-9]'\n          ref={inputRef}\n          type='number'\n          placeholder='Type a number'\n        />\n\n        <button onClick={plus}>ADD</button>\n        <button onClick={minus}>SUBTRACT</button>\n        <button onClick={times}>MULTIPLY</button>\n        <button onClick={divide}>DIVIDE</button>\n        <button onClick={resetInput}>RESET INPUT</button>\n        <button onClick={resetResult}>RESET RESULT</button>\n      </form>\n    </div>\n  )\n}\n\nexport default App","/home/truonganhvu205/Desktop/meta-front-end-developer-professional-certificate/calculator-app-react/src/reportWebVitals.js",[],[],{"ruleId":"19","severity":1,"message":"20","line":32,"column":5,"nodeType":"21","messageId":"22","endLine":34,"endColumn":6},"no-lone-blocks","Nested block is redundant.","BlockStatement","redundantNestedBlock"]